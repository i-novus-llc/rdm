{
  "name": "n2o-config-rdm",
  "version": "1.0.0",
  "description": "N2O конфигурация для проекта RDM",
  "author": "emamoshin",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "npm run clean:commonjs && cross-env NODE_ENV=commonjs babel src --out-dir dist/commonjs --ignore '**/__fixtures__/**/*,src/cosmos.proxies.js'",
    "build:es": "npm run clean:es && cross-env NODE_ENV=es babel src --out-dir dist/es --ignore '**/__fixtures__/**/*,src/cosmos.proxies.js'",
    "clean": "npm run clean:commonjs && npm run clean:es",
    "clean:commonjs": "rimraf dist/commonjs",
    "clean:es": "rimraf dist/es"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "n2o": "7.1.10",
    "n2o-auth": "7.1.10",
    "n2o-data-grid": "1.1.2",
    "react": "^16.6.3",
    "react-autosize-textarea": "^6.0.0",
    "react-avatar": "^3.5.0",
    "react-dom": "^16.6.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-sticky": "^6.0.3",
    "recharts": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.1.4",
    "react-scripts": "2.1.8"
  },
  "files": [
    "dist"
  ]
}
