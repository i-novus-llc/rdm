<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-2.0" name="Операция">
    <form query-id="asyncLog">
        <fields>
            <set field-label-location="left">
                <output-text id="uuid" label="Идентификатор: " label-class="w-50"/>
                <output-text id="tsStart" label="Дата создания: " format="date DD.MM.YYYY HH:mm:ss" label-class="w-50"/>
                <output-text id="tsEnd" label="Дата окончания обработки: " format="date DD.MM.YYYY HH:mm:ss"
                             label-class="w-50"/>

                <output-text id="code" label="Код справочника: " label-class="w-50"/>
                <output-text id="operationTypeDisplay" label="Тип операции: " label-class="w-50"/>
                <output-text id="statusDisplay" label="Статус операции: " label-class="w-50"/>

                <output-text id="error" label="Сообщение об ошибке: " class="text-break" label-class="w-50"/>
            </set>
        </fields>
    </form>
</simple-page>
