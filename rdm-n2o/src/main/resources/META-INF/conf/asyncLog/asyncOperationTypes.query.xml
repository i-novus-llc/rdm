<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0" name="Типы операций">
    <list count-mapping="[totalElements]" result-mapping="[content]">
        <rest>${rdm.backend.path}/asynclog/opTypes</rest>
    </list>
    <fields>
        <field id="id" domain="string">
            <select mapping="toString()"/>
        </field>
        <field id="operationDisplay">
            <select mapping="'Публикация'"/>
        </field>
    </fields>
</query>
