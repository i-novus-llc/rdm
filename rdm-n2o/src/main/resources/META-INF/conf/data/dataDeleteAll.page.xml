<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Удаление всех данных">

    <form>
        <datasource query-id="dataDeleteAll" object-id="data">
            <filters>
                <eq field-id="versionId" param="versionId" model="resolve"/>
                <eq field-id="optLockValue" param="optLockValue" model="resolve"/>
            </filters>
        </datasource>

        <fields>
            <output-text id="message" no-label="true"/>
        </fields>
    </form>
    
</simple-page>
