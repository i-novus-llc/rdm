<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-2.0" name="Создание черновика из файла">
    <form object-id="refBook" default-values-query-id="versionDefault">
        <toolbar place="topLeft">
            <button id="warning" label="Внимание: Все неопубликованные данные будут удалены."
                    color="warning" enabling-condition="false" confirm="false"/>
        </toolbar>
        <fields>
            <file-upload id="file" label="Файл с данными (xml, xlsx)" upload-url="files" label-field-id="name"/>
        </fields>
    </form>
</simple-page>