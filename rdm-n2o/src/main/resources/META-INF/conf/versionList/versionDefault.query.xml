<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">

    <unique result-mapping="#this" filters="versionId">
        <java method="getById">
            <spring bean="versionRestServiceJaxRsProxyClient"/>
            
            <arguments>
                <argument name="versionId" type="primitive" class="java.lang.Integer"/>
            </arguments>
        </java>
    </unique>

    <fields>
        
        <field id="id"/>

        <field id="versionId" domain="integer">
            <select mapping="id"/>
            <filters>
                <eq filter-id="versionId" mapping="['versionId']" domain="integer"/>
            </filters>
        </field>

    </fields>
</query>